/*//member*/

.m-head {
    background: #0ec695;
    display: flex;
    align-items: center;
    padding: 60rpx 40rpx;
}

.m-img {
    margin-right: 20rpx;
}

.m-img image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 200rpx;
}

.m-info {
    color: #fff;
}

.m-list {
    background: #fff;
    padding: 0 55rpx;
}

.m-list>view,.m-list>navigator {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size:30rpx;
    padding: 36rpx 0;
    
    color: #444;
    position: relative;
}
.m-list>view text,.m-list>navigator text{
    color:#888;
}
.m-list>view::before,.m-list>navigator::before {
    content: '';
    position: absolute;
    display: block;
    border-bottom: 1px solid #ddd;
    bottom: 0;
    height: 2px;
    left: -50%;
    right: -50%;
    background-origin: content-box;
    transform: scale(0.5);
    flex-grow:1;
}

.m-list .ico {
    flex-basis:46rpx;
    width:50rpx;
    font-size: 46rpx;
    margin-right:20rpx;
   
}
.m-name{
    flex-grow: 2;
}

.m-list>view:after,.m-list>navigator::after {
    content: '';
    display: block;
    width: 12rpx;
    height: 12rpx;
    border: 2px solid #eee;
    border-left: none;
    border-bottom: none;
    transform: rotate(45deg);
}

.m-list>view:last-child::after {
    border-bottom: none;
}

.m-list .exit:before {
    border: none;
}

.m-ts {
    font-size: 24rpx;
    margin-top: 10rpx;
}

.scroll-report {
    position: absolute;
    left: 0;
    right: 0;
    top: 180rpx;
    bottom: 98rpx;
}

.shop-item {
    display: flex;
    align-items: center;
    background: #fff;
    padding: 20rpx 30rpx;
}

.shop-item:last-child::before {
    border: none;
}

.s-item {
    font-size: 24rpx;
    padding-left: 20rpx;
    border-left: 1px solid #eee;
}

.s-name {
    font-size: 24rpx;
    width: 220rpx;
    color: #888;
    padding-right: 20rpx;
}

.s-title {
    color: #888;
    margin-bottom: 10rpx;
}

.s-num {
    color: #444;
    font-weight: 800;
}

.r-detail>view {
    width: 33.333333%;
    display: inline-block;
    padding: 20rpx 10rpx;
}

.viewall {
    text-align: center;
    font-size: 26rpx;
    padding: 40rpx 0;
    background: #fff;
}

.store {
    text-align: left;
    background: #fff;
    font-size: 24rpx;
    padding: 0 30rpx;
    margin-bottom: -30rpx;
}

.store .picker {
    padding: 28rpx 0;
    font-size: 28rpx;
    color: #444;
    display: flex;
    align-items: center;
}

.store .ico {
    font-size: 32rpx;
    color: #888;
    margin-right: 12rpx;
}

.auth {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 0 40rpx;
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 20%;
}

.auth-txt {
    margin: 0 10%;
    font-size: 28rpx;
    margin-bottom: 40rpx;
    color: #555;
    line-height: 1.6;
    text-align: center;
}

.user-img image {
    width: 140rpx;
    height: 140rpx;
    border-radius: 90rpx;
}

.pxScroll {
    bottom: 160rpx !important;
}

.sm-list {
    padding: 0;
}

.sm-list>view {
    padding: 34rpx 0;
    margin: 0 40rpx;
}

.sm-list>view:last-child::before {
    border: none;
}

.sm-item>view {
    width: 30%;
}

.sm-item>view:first-child {
    width: 40%;
}

.sm-name {
    font-weight: 800;
}

.m-list .phones {
    justify-content: space-between;

}
.m-list .phones text{
    position:absolute;
