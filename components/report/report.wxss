/* pages/temp/report/report.wxss */

/* //retport */

.r {
    background: #fff;
    padding: 10rpx 0;
}

.r-head {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin: 0 30rpx;
}

.r-head>view {
    width: 50%;
    display: flex;
    align-items: center;
    font-size: 24rpx;
    color: #fff;
    padding: 30rpx 20rpx;
}

.r-head>view:first-child {
    border-radius: 8rpx 0 0 8rpx;
}

.r-head>view:last-child {
    border-radius: 0 8rpx 8rpx 0rpx;
}

.r-head>view view {
    font-size: 24rpx;
}

.r-head .ico {
    font-size: 44rpx;
    margin-right: 20rpx;
}

.report {
    padding-bottom: 120rpx;
}

.r-head>view:first-child {
    background: #f56b6b;
}

.r-head>view:last-child {
    background: #658ccf;
}

.r-head>view view view {
    font-size: 32rpx;
}

.datebar {
    display: flex;
    align-items: center;
    background: #fff;
    justify-content: space-between;
    position: relative;
    height: 90rpx;
}

.datebar>view {
    padding: 20rpx 0;
}

.datebar .ico {
    display: flex;
    width: 100rpx;
    align-items: center;
    justify-content: center;
    position: relative;
    z-index: 2;
}

.datebar .ico:after {
    content: '';
    display: block;
    width: 16rpx;
    height: 16rpx;
    border: 2px solid #ccc;
    border-right: none;
    border-top: none;
    transform: rotate(45deg);
}

.datevalue {
    font-size: 28rpx;
    color: #555;
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    text-align: center;
    z-index: 0;
}

.datebar .next {
    transform: rotate(180deg);
}

.r-grid {
    overflow: hidden;
    background: #fff;
    margin: 0 30rpx;
}

.r-grid .ico {
    font-size: 44rpx;
    margin-right: 20rpx;
    margin-left: 20rpx;
}

.r-grid .value {
    font-size: 32rpx;
    color: #444;
    margin-top: 10rpx;
}

.r-grid>view {
    display: flex;
    align-items: center;
    width: 50%;
    float: left;
    font-size: 24rpx;
    padding: 32rpx 0;
    color: #777;
}

.r-title {
    padding: 0 20rpx;
    font-size: 28rpx;
    color: #0ec695;
    margin: 50rpx 0 20rpx;
    border-left: 20rpx solid #0ec695;
    line-height: 28rpx;
}

.rs-block {
    padding: 20rpx 0;
    overflow: hidden;
}

.rs-block>view {
    width: 33.3333333333%;
    float: left;
    font-size: 24rpx;
    color: #666;
    padding: 10rpx 0;
    position: relative;
    margin: 10rpx 0;
    text-indent: 30rpx;
}

.rs-title {
    padding: 0 30rpx;
    font-size: 28rpx;
    display: flex;
    align-items: center;
    padding-top: 30rpx;
}

.rs-title .ico {
    font-size: 46rpx;
    margin-right: 14rpx;
}

.rs-title .weixin {
    color: #23b300;
}

.rs-title .alipay {
    color: #1394ff;
}

.rs-block>view:after {
    content: '';
    border-right: 1px solid #eee;
    display: block;
    position: absolute;
    right: 0;
    top: 0;
    bottom: 0;
}

.rs-block>view text {
    font-size: 28rpx;
    display: block;
    line-height: 2;
    color: #444;
}

.rs {
    background: #fff;
}

/*//member*/

.m-head {
    background: #0ec695;
    display: flex;
    align-items: center;
    padding: 60rpx 40rpx;
}

.m-img {
    margin-right: 20rpx;
}

.m-img image {
    width: 90rpx;
    height: 90rpx;
    border-radius: 200rpx;
}

.m-info {
    color: #fff;
}

.m-list {
    background: #fff;
    padding: 0 55rpx;
}

.m-list>view {
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 28rpx;
    padding: 36rpx 0;
    padding-left: 70rpx;
    color: #444;
    position: relative;
}

.m-list>view:before {
    content: '';
    position: absolute;
    display: block;
    border-bottom: 1px solid #ddd;
    bottom: 0;
    height: 2px;
    left: -50%;
    right: -50%;
    background-origin: content-box;
    transform: scale(0.5);
}

.m-list .ico {
    position: absolute;
    left: 0;
    top: 50%;
    margin-top: -20rpx;
    font-size: 54rpx;
}

.m-list>view:after {
    content: '';
    display: block;
    width: 12rpx;
    height: 12rpx;
    border: 2px solid #eee;
    border-left: none;
    border-bottom: none;
    transform: rotate(45deg);
}

.m-list>view::last-child::after {
    border-bottom: none;
}

.m-list .exit:before {
    border: none;
}

.m-ts {
    font-size: 24rpx;
    margin-top: 10rpx;
}

.o-item {
    display: flex;
    align-items: center;
    justify-content: space-between;
    background: #fff;
    padding: 20rpx 30rpx;
    position: relative;
}

.o-item:after {
    content: '';
    display: block;
    height: 1px;
    left: -50%;
    right: -50%;
    bottom: 0;
    position: absolute;
    border-bottom: 1px solid #eee;
    transform: scale(0.5);
}

.o-bar {
    display: flex;
    background: #fff;
}

.o-head {
    background: #0ec695;
    color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 24rpx;
}

.o-head text {
    display: block;
    font-size: 30rpx;
}

.typeico {
    font-size: 22rpx;
    width: 80rpx;
    height: 80rpx;
    border: 2px solid #ccc;
    border-radius: 200rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: centerl;
    opacity: 0.7;
    transform: scale(0.9);
}

.o-type .typeico text {
    text-align: center;
    border: 1px solid #ccc;
    position: absolute;
    left: -4px;
    right: -4px;
    top: 50%;
    line-height: 38rpx;
    height: 38rpx;
    margin-top: -19rpx;
    background: #fff;
    border-radius: 3px;
    z-index: 2;
    font-weight: 700;
    color: #ccc;
    transform: rotate(-45deg);
}

.bline {
    position: relative;
}

.bline:after {
    content: '';
    display: block;
    height: 1px;
    left: -50%;
    right: -50%;
    bottom: 0;
    position: absolute;
    border-bottom: 1px solid #eee;
    transform: scale(0.5);
}

.rline, .bline {
    position: relative;
}

.rline:before {
    content: '';
    display: block;
    width: 1px;
    top: -50%;
    right: 0;
    bottom: -50%;
    position: absolute;
    border-right: 1px solid #eee;
    transform: scale(0.5);
}

.typeico:after {
    content: '';
    display: block;
    position: absolute;
    border-radius: 200rpx;
    border: 1px solid #ccc;
    left: 2px;
    top: 2px;
    bottom: 2px;
    right: 2px;
}

.o-head>view {
    width: 33.33333333%;
    padding: 20rpx 15rpx;
}

.o-bar>view {
    width: 33.333333%;
    text-align: center;
    font-size: 24rpx;
    padding: 28rpx 0;
    color: #444;
    display: flex;
    align-items: center;
    justify-content: center;
}

.o-bar>view:after {
    content: '';
    display: block;
    width: 10rpx;
    height: 10rpx;
    border: 2px solid #ddd;
    border-top: none;
    border-right: none;
    transform: rotate(-45deg);
    margin-left: 30rpx;
}

.o-type {
    font-size: 80rpx;
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
    text-align: center;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
}

.o-type .ico {
    width: 70rpx;
    height: 70rpx;
    color: #fff;
    font-size: 50rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    border-radius: 6rpx;
    margin-bottom: 4rpx;
}

.o-type .ico:before {
    margin-top: -1px;
}

.o-type .weixin {
    background-color: #23b300;
}

.o-type .alipay {
    background-color: #1394ff;
}

.o-type .vip {
    background-color: tomato;
}

.o-list {
    background: #fff;
}

.o-line {
    overflow: hidden;
    text-overflow: ellipsis;
    margin-bottom: 20rpx;
    color: #999;
}

.o-type text {
    font-size: 22rpx;
    display: block;
    color: #666;
}

.o-no {
    font-size: 24rpx;
    width: 100%;
    color: #444;
}

.o-price {
    width: 130rpx;
    font-size: 24rpx;
    text-align: right;
}

.o-title {
    font-size: 28rpx;
    margin-bottom: 8rpx;
}

.o-amt {
    font-size: 32rpx;
    color: #f56b6b;
}

.o-date {
    color: #888;
}

.o-tag {
    margin-top: 10rpx;
    color: #666;
}

.scroll-order {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 120rpx;
    background: #fff;
}

.getmore {
    font-size: 26rpx;
    text-align: center;
    line-height: 2;
    padding: 40rpx 0;
}

.shop-item {
    display: flex;
    align-items: center;
    background: #fff;
    padding: 20rpx 30rpx;
}

.shop-item:last-child::before {
    border: none;
}

.s-item {
    font-size: 24rpx;
    padding-left: 20rpx;
    border-left: 1px solid #eee;
}

.s-name {
    font-size: 24rpx;
    width: 220rpx;
    color: #888;
    padding-right: 20rpx;
}

.s-title {
    color: #888;
    margin-bottom: 10rpx;
}

.s-num {
    color: #444;
    font-weight: 800;
}

.r-detail>view {
    width: 33.333333%;
    display: inline-block;
    padding: 20rpx 10rpx;
}

.viewall {
    text-align: center;
    font-size: 26rpx;
    padding: 40rpx 0;
    background: #fff;
}

.store {
    text-align: center;
    background: #fff;
    font-size: 24rpx;
}
.r-info{
    font-size:23rpx;
    color:#999;
    display:flex;
    margin:20rpx 30rpx 0;
    align-items:center;
}
.r-info .ico{
    margin-right:10rpx;
    color:#0EC695;
    transform: rotate(180deg);
    font-size:32rpx;
}
